    def leadership2(p,q):
        import pymysql as pm
        conn=pm.connect(host="localhost",user="root",password="divyanshisql",database="gamea")
        cur=conn.cursor()
        
        t=name
        if p>q:
            s="You win ğŸ˜„"
        elif p==q:
            s="Tie"
        else:
            s="You lost ğŸ™"
        
        query="insert into gamerb values(%s,%s,%s,%s)"
        val=t,p,q,s
        cur.execute(query,val)
        conn.commit()
        query1=cur.execute("select * from gamerb;")
        lb=cur.fetchall()
        print()
        print("â•‘  NAME    â•‘    ğ•Œğ•Šğ”¼â„ â„™ğ•†ğ•€â„•ğ•‹  â•‘  â„‚ğ•†ğ•„â„™ğ•Œğ•‹ğ”¼â„'ğ•Š â„™ğ•†ğ•€â„•ğ•‹  â•‘    â„ğ”¼ğ•Šğ•Œğ•ƒğ•‹  ")

